function [ latin ] = latinSquare( N, S )
%LATINSQUARE Summary of this function goes here
%   Detailed explanation goes here

    latin = zeros(N,S);
    seq = [1:S];
    	
    order = randperm(N);
    while order(1) == 1
        order = randperm(N);
    end
    
    for i=1:S
        start = order(i); 
        latin(:,i) = [seq(start:N) seq(1:order(i)-1))];
    end
end

